<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.quantil.cm.api.mapper.PurgeTaskStatusMapper">

    <select id="getPurgeTaskStatusByTaskId" resultType="com.quantil.cm.api.domain.PurgeTaskStatus">
        SELECT *
          FROM purge_task_status
          WHERE task_id = #{taskId}
    </select>
    
</mapper>